{
  "extends": [ "eslint:recommended", "plugin:react/recommended" ],
  "env": {
    "node": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": 0
  },
  "parserOptions": {
      ecmaVersion: 6,
      ecmaFeatures: {
          "jsx": true
      },
      sourceType: "module"
  },
  "plugins": ["react"]
}